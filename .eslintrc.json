{
    "env": {
        "browser": true,
        "es2021": true,
        "es6": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "ecmaVersion": 6,
            "es6": true,
            "jsx": true
        },
        "ecmaVersion": 13,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {      
      "react/destructuring-assignment": ["off", "never"],
        "no-param-reassign": 0,
        "no-unused-vars": "off",
        "import/extensions": "off",
        "import/no-unresolved": "off",
        "react/prop-types": "off",
        "react/jsx-filename-extension":[0],
        "react/button-has-type": ["off", {
            "button": true,
            "submit": true,
            "reset": true
          }]
          ,"no-console": "off"

    }
}
